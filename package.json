{
  "name": "wordup-cli",
  "description": "Development toolkit for setting up and managing your local WordPress Theme/Plugin",
  "version": "0.4.0",
  "bin": {
    "wordup": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1.5.15",
    "@oclif/config": "^1.13.0",
    "@oclif/plugin-help": "^2.2.0",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "cli-ux": "^5.3.0",
    "dot-prop": "^5.1.0",
    "express": "^4.17.1",
    "fs-extra": "^7.0.1",
    "inquirer": "^6.4.1",
    "open": "^6.3.0",
    "shelljs": "^0.8.3",
    "slugify": "^1.3.4",
    "update-notifier": "^3.0.0",
    "yaml": "^1.6.0",
    "yeoman-environment": "^2.4.0",
    "yeoman-generator": "^3.2.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^8.0.2",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src",
    "/docker",
    "/templates"
  ],
  "homepage": "https://wordup.dev",
  "repository": "github:wordup-dev/wordup-cli",
  "bugs": "https://github.com/wordup-dev/wordup-cli/issues",
  "keywords": [
    "wordpress",
    "wordup",
    "devtools",
    "php",
    "oclif",
    "cli",
    "docker"
  ],
  "license": "GPL-3.0",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "wordup",
    "hooks": {
      "init": "./src/hooks/update"
    },
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md",
    "postversion": "git push && git push --tags"
  }
}
